<td data-test-song-name>
  {{model.name}}
</td>
<td data-test-song-artist>
  {{#if model.artist}}
  <ul>
    {{#each model.artist as |artist|}}
      <li>
        {{artist.name}}
      </li>
    {{/each}}
  </ul>
  {{/if}}
</td>
<td data-test-song-genre>
  {{model.genre.name}}
</td>
<td data-test-song-likes>
  {{model.likes}}
</td>
